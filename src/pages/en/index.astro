---
import HomePageLayout from "@ui/home/layout/HomePageLayout.astro";
import { useTranslations } from "@i18n/utils";
import { CONTACT_INFO } from "@ui/constants/contact";

const lang = "en";
const t = useTranslations(lang);
---

<HomePageLayout
  title={t('seo.title')}
  description={t('seo.description')}
  lang={lang}
  keywords={t('seo.keywords')}
  canonicalPath="/en/"
  whatsappPhone={CONTACT_INFO.whatsappPhone}
/>
